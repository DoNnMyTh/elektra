- if @auto_approve_success
  .alert.alert-info
    %p
      Your quota request for
      %strong=@resource.name
      was auto approved. New value is
      %strong=@resource.data_type.format(@resource.quota)
- if @send_resource_request
  .alert.alert-info
    %p
      Your quota request for
      %strong=@resource.name
      was successfully send.
- if @reduce_resource_success
  .alert.alert-info
    %p
      Your quota for
      %strong=@resource.name
      was successfully reduces to
      %strong=@resource.data_type.format(@resource.quota)
