- data_type = domain_resource.data_type

%tr
  %td.col-md-3
    = domain_resource.domain_id
  %td.col-md-3
    = data_type.format(domain_resource.approved_quota)
  %td.col-md-2
    = data_type.format(domain_resource.current_quota)
  %td.col-md-2
    = data_type.format(domain_resource.usage)
  %td.col-md-2
    .resource-actions
      = link_to 'Edit', '#', class: 'btn btn-primary btn-sm', remote: true

