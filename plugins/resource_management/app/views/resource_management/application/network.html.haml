= render :partial => 'header'
%ul{:class => "nav nav-tabs"}
  = render :partial => 'tabs', :locals => { :tab => 'network'}

.container
  .row.quota-row
    .col-md-9
      - unless @network_quotas.nil? or @network_quotas.blank?
        - @network_quotas.each do |resource_data|
          = render :partial => 'bar', :locals => { :overview => false,  :data => resource_data }
      - else
        .bs-callout
          No Network Quotas found!

    .col-md-3
      = render :partial => 'good_to_know'
