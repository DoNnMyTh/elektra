<% unless @automations.nil? %>
$(".js-table-automations").html("<%= escape_javascript(render partial: 'table_automations', locals: {automations: @automations}) %>");
$(".js-table-params").data("automation-page", "<%= @pag_params[:automation][:page] %>");
<% end %>

<% unless @runs.nil? %>
$(".js-table-runs").html("<%= escape_javascript(render partial: 'table_runs_pagination') %>");
$(".js-table-params").data("run-page", "<%= @pag_params[:run][:page] %>");
<% end %>

var params = { pag_params: {automation: {page: $(".js-table-params").data("automation-page")}, run: {page: $(".js-table-params").data("run-page")}} };
console.log(params)
history.replaceState(null, "", location.pathname + "?" + $.param( params ));

// reload the pagination spinner
$('[data-toggle="paginationSpinner"]').initPaginationSpinner()
