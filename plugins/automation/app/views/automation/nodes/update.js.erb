$(".js-inline-form-node-tags").html("<%= escape_javascript( render partial: 'form_tags', locals: {node_form: @node_form, node_form_read: @node_form_read} ) %>");
<% if @error%>
  $('.js-inline-form-node-tags').initInlineFormNodeTag({state: 'open'})
<% else %>
  $('.js-inline-form-node-tags').initInlineFormNodeTag()
<% end %>